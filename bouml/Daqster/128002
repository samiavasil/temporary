	  param in name "msgId" explicit_type "msg_id_t"
	  param in name "data" explicit_type "u8"
	constraint "int ret;                                               
int msgLen;                                            
int offset;                                            
if( ( NULL == packet )||( NULL == data ) ){            
    return WRONG_PARAMS;                               
}                                                      
ret = getMessageBitLen( msgId, &msgLen );              
if( NO_ERR == ret ){                                   
    ret = packet->setBits( offset, msgLen, data );     
}                                                      
return ret;"
